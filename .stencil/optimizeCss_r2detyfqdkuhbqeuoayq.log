html {
  font-family: "Raleway", "Helvetica", sans-serif;
  font-size: 100%;
  background-color: #fdfffc;
  color: #0a0908;
  height: 100%;
  width: 100%;
}

#body {
  height: 100%;
  width: 100%;
  background-repeat: no-repeat;
  background-attachment: fixed;
}

:host {
  width: 100%;
}

@media only screen and (max-width: 600px) {
  pro-modal {
    width: 50%;
    height: 50%;
  }
}
pro-card {
  display: block;
  position: relative;
  overflow: hidden;
  -webkit-box-shadow: 0 1px 3px #b7b7b7;
  box-shadow: 0 1px 3px #b7b7b7;
  -webkit-transition: 0.3s;
  transition: 0.3s;
  border-color: #0a0908;
  border-width: 1px;
  font-size: 16px;
  padding: 5px;
  margin: 5px;
  background-color: #fdfffc;
  min-height: 200px;
  max-height: 500px;
}