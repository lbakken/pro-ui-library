@import url("https://fonts.googleapis.com/css?family=Raleway&display=swap");
.good-weather {
  background: #fcd581;
  background-image: -webkit-gradient(linear, left bottom, left top, from(#fcd581), color-stop(65%, #fdfffc));
  background-image: linear-gradient(0deg, #fcd581 0%, #fdfffc 65%);
  background-repeat: no-repeat;
  background-attachment: fixed;
  height: 100%;
  color: #0a0908;
}

.bad-weather {
  background: #0a0908;
  background: -webkit-gradient(linear, left bottom, left top, from(#0a0908), color-stop(65%, #263a3c));
  background: linear-gradient(0deg, #0a0908 0%, #263a3c 65%);
  background-repeat: no-repeat;
  background-attachment: fixed;
  height: 100%;
  color: #fdfffc;
}

