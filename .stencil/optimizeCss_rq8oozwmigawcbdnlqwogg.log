  color: #0a0908;
  height: 100%;
  width: 100%;
}

#body {
  height: 100%;
  width: 100%;
  background-repeat: no-repeat;
  background-attachment: fixed;
}

:host {
  width: 100%;
}

@media only screen and (max-width: 600px) {
  pro-modal {
    width: 50%;
    height: 50%;
  }
}
:host .button-native {
  background-color: #263a3c;
  padding: 5px;
  border-radius: 10%;
  padding-top: 10px;
  border-radius: 5px;
  color: #fdfffc;
  font-family: "Raleway", "Helvetica", sans-serif;
  font-size: 14px;
  text-transform: uppercase;
  width: 100%;
  min-height: 35px;
  max-height: 70px;
  -webkit-transition: 0.5s;
  transition: 0.5s;
  cursor: pointer;
}
:host .button-inner {
  font-weight: bold;
}
:host button:disabled {
  background-color: gray;
  color: white;
  cursor: auto;
  font-size: 14px;
}
:host :hover {
  background-color: #bdeaf9;
  color: #263a3c;
  -webkit-transition: 0.5s;
  transition: 0.5s;
  font-size: 16px;
}
:host :focus {
  outline: none;
  -webkit-box-shadow: 0 1px 3px #b7b7b7;
  box-shadow: 0 1px 3px #b7b7b7;
}
@media only screen and (max-width: 500px) {
  :host .button-native {
    font-size: 10px;
  }
  :host :hover {
    font-size: 12px;
  }
  :host button:disabled {
    font-size: 10px;
  }
}