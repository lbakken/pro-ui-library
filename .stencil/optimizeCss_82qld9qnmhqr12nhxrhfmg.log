input[type=checkbox]:after {
  width: 15px;
  height: 15px;
  border-radius: 2px;
  top: -2px;
  left: -1px;
  position: relative;
  background-color: gainsboro;
  content: "";
  display: inline-block;
  visibility: visible;
  border: 2px solid white;
}

input[type=checkbox]:checked:after {
  width: 15px;
  height: 15px;
  border-radius: 2px;
  top: -2px;
  left: -1px;
  content: "âœ”";
  position: relative;
  display: inline-block;
  visibility: visible;
  border: 2px solid white;
  color: #30bced;
  text-align: center;
}

input[type=checkbox]:hover:after {
  background: #1e96ff;
  -webkit-transition: 1s;
  transition: 1s;
}