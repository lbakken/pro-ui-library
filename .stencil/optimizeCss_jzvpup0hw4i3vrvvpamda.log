:host {
  display: block;
  -o-object-fit: contain;
  object-fit: contain;
}
:host img {
  display: block;
  min-width: 50px;
  min-height: 50px;
  max-width: 150px;
  max-height: 150px;
  width: 100%;
  height: 100%;
  -o-object-fit: inherit;
  object-fit: inherit;
  -o-object-position: inherit;
  object-position: inherit;
}